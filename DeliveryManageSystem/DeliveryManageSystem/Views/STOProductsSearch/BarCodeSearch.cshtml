                    @Html.DisplayFor(modelItem => item.IngredientCN)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IngredientJP)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Price)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NetWt)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.GrossWeight)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NetWeight)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ULR)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OriginCN)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OriginJP)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Factory)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BrandCN)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BrandJP)
                </td>
            </tr>
        }

    </table>
